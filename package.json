{
  "name": "speedvitals",
  "version": "0.1.3",
  "description": "Speedvitals CLI",
  "license": "MIT",
  "author": "Speedvitals",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "speedvitals": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "prepublishOnly": "npm run build",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@types/env-ci": "^3.1.4",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "eslint": "^9.36.0",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "commander": "^14.0.1",
    "env-ci": "^11.2.0",
    "zod": "^4.1.11"
  },
  "keywords": [
    "speedvitals-cli",
    "web-vitals testing",
    "performance budgets",
    "lighthouse"
  ]
}
